{
    "subpath": "/",
    "listen": "localhost:8000",
    "max_file_size": 20971520,
    "use_X-Real-IP_header": false,
    "debug": false,
    "random_img": false,
    "log_http_requests": true,

    "rate_limit_count": 8,
    "rate_limit_interval_seconds": 60,

    "encrypted_file_lifetime_minutes": 60,

    "accepted_fmt": {
        "application/octet-stream": "i9k",
        "application/ogg": "ogg",
        "application/x-zip-compressed": "zip",
        "application/zip": "zip",
        "application/x-gzip": "gz",
        "application/pdf": "pdf",
        "audio/wav": "wav",
        "audio/x-wav": "wav",
        "audio/flac": "flac",
        "audio/mp3": "mp3",
        "audio/mpeg": "mp3",
        "audio/ogg": "ogg",
        "image/gif": "gif",
        "image/jpeg": "jpg",
        "image/png": "png",
        "image/svg+xml": "svg",
        "text/plain; charset=utf-8": "txt",
        "text/xml; charset=utf-8": "xml",
        "image/heif": "heic",
        "image/heic": "heic",
        "image/heif-sequence": "heic",
        "image/heic-sequence": "heic",
        "video/mp4": "mp4",
        "video/ogg": "ogv",
        "video/webm": "webm",
        "video/x-matroska": "mkv"
    },

    "accepted_text_fmt": [
        "css",
        "obj",
        "js",
        "json",
        "xml",
        "txt",
        "sh",
        "lua",
        "go"
    ],

    "sassy_remarks": [
        "hello darkness my old friend",
        "open a bug report if this doesn't work",
        "how did it come to this",
        "deleted code is debugged code.",
        "i'm only human, I too make horrible mistakes",
        "dear god make it stop",
        "this clearly isn't professional",
        "holy fuck this uploader sucks",
        "remember to stop uploading every now and again. uploading for extended periods is unhealthy",
        "if this doesn't work please open an issue on github"
    ]
}
